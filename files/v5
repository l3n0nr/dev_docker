#### APACHE
FROM debian

MAINTAINER lenonr

RUN apt update && \
    apt install apache2 nano -y &&
    apachectl start

EXPOSE 80

#### docker run -tip 80:80 -v /home/lenonr/Github/dev_web/SAFD:/var/www/html apache /bin/bash

